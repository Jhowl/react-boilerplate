{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A simple react boilerplate",
  "main": "dist/server/index.js",
  "author": "Paulo Ferreira",
  "scripts": {
    "start": "yarn prod-start",
    "serve": "yarn dev",
    "dev": "yarn nodemon",
    "nodemon": "cross-env NODE_ENV=development nodemon src/server/index.js --watch src/server/",
    "build": "yarn build:client && yarn build:server",
    "build:client": "cross-env NODE_ENV=production BABEL_ENV=production webpack --progress --config webpack/prod.config -p",
    "build:server": "cross-env NODE_ENV=production BABEL_ENV=production babel src -d dist --ignore=src/client,src/components/1.styleguide,mock,.scss,story.js,spec.js --source-maps",
    "prod-start": "PORT=5600 NODE_ENV=production BABEL_ENV=production forever start dist/server/index.js",
    "prod-stop": "forever stopall"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "forever": "^0.15.3",
    "html-webpack-plugin": "^2.30.1",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.7.2",
    "prismjs": "^1.11.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-element-to-jsx-string": "^13.1.0",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "sass-to-js": "^1.3.1",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "base-64": "^0.1.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-id-swiper": "^1.5.8",
    "react-player": "^1.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
